<?xml version='1.0' encoding='utf-8'?>
<widget id="com.codearmada.biobebe" version="0.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>BioBebe</name>
    <description>
		An app for babies and mothers.
	</description>
    <author email="info@codearmada.com" href="http://codearmada.com">
		CODEARMADA limited
	</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/android/ldpi.png" />
        <icon density="mdpi" src="res/android/mdpi.png" />
        <icon density="hdpi" src="res/android/hdpi.png" />
        <icon density="xhdpi" src="res/android/xhdpi.png" />
        <icon density="xxhdpi" src="res/android/xxhdpi.png" />
        <icon density="xxxhdpi" src="res/android/xxxhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="res/ios/apple-icon-180x180.png" width="180" />
        <icon height="60" src="res/ios/apple-icon-60x60.png" width="60" />
        <icon height="120" src="res/ios/apple-icon-120x120.png" width="120" />
        <icon height="76" src="res/ios/apple-icon-76x76.png" width="76" />
        <icon height="152" src="res/ios/apple-icon-152x152.png" width="152" />
        <icon height="40" src="res/ios/apple-icon-57x57.png" width="40" />
        <icon height="80" src="res/ios/apple-icon-114x114.png" width="80" />
        <icon height="57" src="res/ios/apple-icon-57x57.png" width="57" />
        <icon height="114" src="res/ios/apple-icon-114x114.png" width="114" />
        <icon height="72" src="res/ios/apple-icon-72x72.png" width="72" />
        <icon height="144" src="res/ios/apple-icon-144x144.png" width="144" />
        <icon height="29" src="res/ios/apple-icon-57x57.png" width="29" />
        <icon height="58" src="res/ios/apple-icon-57x57.png" width="58" />
        <icon height="50" src="res/ios/apple-icon-57x57.png" width="50" />
        <icon height="100" src="res/ios/apple-icon-114x114.png" width="100" />
        <icon height="167" src="res/ios/apple-icon-180x180.png" width="167" />
    </platform>
    <plugin name="cordova-plugin-file" spec="~4.3.3" />
    <plugin name="cordova-sqlite-ext" spec="~0.10.4" />
    <plugin name="cordova-plugin-dbcopy" spec="~1.0.4" />
</widget>
